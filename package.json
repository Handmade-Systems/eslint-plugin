{
  "name": "@handmade-systems/eslint-plugin",
  "version": "0.0.10",
  "description": "shared eslint config for handmade systems",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Handmade Systems",
  "main": "index.js",
